        "jeremytubbs/deepzoom": "0.6.*"
    },
    "require-dev": {
        "larapack/dd": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jeremytubbs\\LaravelDeepzoom\\DeepzoomServiceProvider"
            ]
        }
    }
}
