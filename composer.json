{
    "name": "jeremytubbs/laravel-deepzoom",
    "description": "Laravel Deepzoom Tile Generator for OpenSeadragon",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Tubbs",
            "email": "jeremytubbs@gmail.com",
            "homepage": "https://jeremytubbs.com"
        }
    ],
    "keywords": [
        "laravel",
        "openseadragon",
        "deepzoom",
        "dzi",
        "zoom",
        "zoomable",
        "tiled"
    ],
    "autoload": {
